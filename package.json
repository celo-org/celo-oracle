{
  "name": "celo-oracle",
  "version": "0.0.0",
  "description": "Oracle application to aggregate and report exchange rates to the Celo network",
  "author": "Celo",
  "license": "Apache-2.0",
  "repository": "git@github.com:celo-org/celo-oracle.git",
  "private": true,
  "scripts": {
    "build": "tsc -b .",
    "clean": "tsc -b . --clean",
    "start": "node lib/index.js",
    "test": "jest",
    "lint": "tslint -c tslint.json --project .",
    "lint:tests": "tslint -c tslint.json 'test/**/*.test.ts'",
    "prettify": "yarn run prettier --config .prettierrc.js --write '**/*.+(ts|tsx|js|jsx|sol|java)'",
    "prettify:diff": "yarn run prettier --config .prettierrc.js --list-different '**/*.+(ts|tsx|js|jsx|sol|java)'"
  },
  "dependencies": {
    "@celo/connect": "^1.0.0-beta3",
    "@celo/contractkit": "1.0.0-beta3",
    "@celo/utils": "^0.1.22",
    "@celo/wallet-hsm-aws": "^1.0.0-beta3",
    "@celo/wallet-hsm-azure": "^1.0.0-beta3",
    "bignumber.js": "^8.1.1",
    "bunyan": "^1.8.12",
    "express": "^4.17.1",
    "mathjs": "^6.6.4",
    "node-fetch": "^2.6.1",
    "prom-client": "^12.0.0",
    "tslint-no-focused-test": "^0.5.0",
    "web3": "1.2.4"
  },
  "devDependencies": {
    "@celo/typescript": "^0.0.1",
    "@types/bunyan": "^1.8.6",
    "@types/express": "^4.17.6",
    "@types/jest": "^25.1.4",
    "@types/mathjs": "^6.0.5",
    "@types/node-fetch": "^2.5.5",
    "jest": "^25.1.0",
    "prettier": "^2.0.5",
    "ts-jest": "^25.2.1",
    "tslint": "^6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "tslint-react": "^4.2.0",
    "tslint-react-hooks": "^2.2.2",
    "typescript": "^3.8.3"
  }
}
